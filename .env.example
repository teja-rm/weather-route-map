# Environment Variables for Route Map App
# Copy this file to .env and add your actual API keys

# HERE Maps API Key (required for routing)
HERE_API_KEY=your_here_api_key_here

# Mapbox API Key (required for geocoding)
MAPBOX_API_KEY=your_mapbox_api_key_here

# OpenCage API Key (required for geocoding)
OPENCAGE_API_KEY=your_opencage_api_key_here

# OpenWeatherMap API Key (required for weather data)
OPENWEATHER_API_KEY=your_openweather_api_key_here
